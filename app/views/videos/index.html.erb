<div class="container">
<h1>Videos</h1>
<% @videos.each do |video| %>
<ul>
  <li>
    <%= video.title %>


   <div class="video-player">
      <iframe width="340" height="140" src="<%= video.url %>" frameborder="0" allow="accelerometer; autoplay; encrypted-media; gyroscope; picture-in-picture" allowfullscreen></iframe>
    </div>

<%= link_to "  PLAY", video_path(video), class: "btn btn-primary fas fa-play" %>
  </li>
</ul>
<% end %>

</div>


